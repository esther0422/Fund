@import "reset"
$green=rgb(0,204,206)
.main
	position relative
	margin-right auto
	margin-left auto
	width 1190px

.menu
	border-bottom 2px solid $green
	height 36px
	.left
		float left
	.right
		float right
	.anchor
		position relative
		float left
		padding-right 40px
		padding-left 40px
		font-size 14px
		line-height 34px
		border-bottom-width 2px 
		border-bottom-style solid 
		border-bottom-color transparent
		cursor pointer
		&:not(:last-child)
			border-right 1px solid rgb(230,230,230)
		&.current
			border-bottom @border-bottom
		&:not(div)
		> a
			&:hover	
				color $green
		> a
			&:hover
				&+.fixedBox
					.items
						transform translateY(0)
				&:after
					background-image url("data:image/png;base64,")
					transform rotate(180deg)
			&:after
				margin-left 3px
				display inline-block
				width 11px
				height 8px
				vertical-align middle
				content ""
				transition transform .25s
				background-image url("data:image/png;base64,")			
		.fixedBox
			position absolute
			z-index 1
			top 26px
			left 50%
			transform translateX(-50%)
			overflow hidden
			&:hover
				.items
					transform translateY(0)
			.items
				width 122px
				background-color white
				overflow hidden
				transform translateY(-100%)
				border 1px solid rgb(220,220,220)
				transition transform .5s
			a
				float left
				width 100%
				height 40px
				border-bottom 1px dotted rgb(220,220,220)
				text-align center
				line-height 40px
				color black
				&:hover
					background-color $green
					color white
		
.firstScreen
	margin-bottom 140px
	height 540px
	background-color rgb(242,242,242)
	.left
		float left
		margin-top 20px
		margin-bottom 20px
		width 890px
		height 500px
		background-color red
		.banner
			float left
			width 540px
			height @height
		.ad
			float right
			width 350px
			height (@height/2)
	.right
