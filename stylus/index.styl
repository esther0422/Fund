		.item
			width 290px
			height 194px
			background-color pink
			a
				width @width
				height @height
				display inline-block
				img
					width @width
					height @height
			.showing
				display block
			.hiddened
				display none
	.bottom
		float left
		width 900px
		height 140px
		li
			width (@width/4)
			float left
			text-align center
			position relative
			span
				margin-top 48px
				display inline-block
				font-size 12px
				color rgb(153,153,153)
			strong
				display block
				font-size 30px
				margin-top 6px
				color rgb(102,102,102)
			.line
				position absolute
				top -8px
				right 0px
				height 65px
				border-right 1px solid $green
				transform rotate(20deg)			
.hot
	height 634px
	background-image url("../images/hot.png")
	background-position 50% 50%
	.title
		display inline-block
		color white
		font-size 28px
		margin-top 36px
		margin-left 540px
	.tags
		text-align center
		margin-top 20px
		.Text
			display inline-block
			color white
			font-size 14px
		.tag
			display inline-block
			border 1px solid white
			border-radius 3px 3px 3px 3px
			padding 8px 12px
			color white
			font-size 12px
			margin-left 10px
			&:hover
				cursor pointer
				background-color $green
				border-color $green
	.left-icon
		position absolute
		left -55px
		top 320px
		display inline-block
		width 50px
		height 50px
		font-size 48px
		color rgb(166,166,166)
		cursor pointer
		display none
		&:hover
			color $green
	.right-icon
		position absolute
		left 1196px
		top 320px
		display inline-block
		width 50px
		height 50px
		font-size 48px
		color rgb(166,166,166)
		cursor pointer
		&:hover
			color $green
	.hot-content
		position relative
		width 1190px
		height 450px
		margin-top 20px
		overflow hidden
		.showBox
			width 2000px
			height 450px
			position absolute
			left 0
			transition left .5s
		.show:nth-of-type(1)
			margin-left 0 
		.pic
			position relative
			left 0
			top 0
		.show
			float left
			width 222px
			height 450px
			background-color white
			margin-left 20px
			cursor pointer
		.show
			&:nth-of-type(1)
				background-image url("../images/hot1.jpg")
			&:nth-of-type(2)
				background-image url("../images/hot2.jpg")
			&:nth-of-type(3)
				background-image url("../images/hot3.png")
			&:nth-of-type(4)
				background-image url("../images/hot4.jpg")
			&:nth-of-type(5)
				background-image url("../images/hot5.jpg")
			&:nth-of-type(6)
				background-image url("../images/hot6.png")
			&:nth-of-type(7)
				background-image url("../images/hot7.jpg")
			&:nth-of-type(8)
				background-image url("../images/hot8.jpg")
			.item-title
				color rgb(42,52,63)
				font-size 24px
				text-align center
				line-height 60px
			.notate	
				color rgb(153,156,180)
				font-size 12px
				margin-top 16px
				margin-left 20px
			.rate
				margin-left 20px
				margin-top 50px
				p:nth-of-type(1)
					color rgb(130,140,152)
					font-size 18px
				p:nth-of-type(2)
					color $green
					font-size 26px
					margin-top 10px
				
