		margin-top 20px
		width 290px
		height 660px
		background-color pink
		.ban
			width 290px
			height 78px
			background-color $green
			line-height @height
			.text
				font-size 28px
				color white
				margin-left 20px
			span
				float right
				margin-right 20px
				margin-top 22px
				text-align center
			.change
